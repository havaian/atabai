{
  "name": "atabai-frontend",
  "version": "1.0.0",
  "description": "ATABAI Frontend - Excel automation platform for IFRS compliance",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 8080",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "vue3",
    "excel",
    "ifrs",
    "financial",
    "frontend",
    "tailwind",
    "spreadsheet",
    "automation"
  ],
  "author": "ATABAI Team",
  "license": "MIT",
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.2.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.7",
    "lucide-vue-next": "^0.441.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.1.1",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0",
    "vue-i18n": "^9.9.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/node": "^20.14.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4",
    "vue-tsc": "^2.0.21"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}