{
  "name": "atabai-frontend",
  "version": "1.0.0",
  "description": "ATABAI Frontend - Excel automation platform for IFRS compliance",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 8080",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "vue3",
    "excel",
    "ifrs",
    "financial",
    "frontend",
    "tailwind",
    "spreadsheet",
    "automation"
  ],
  "author": "ATABAI Team",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/forms": "^0.5.7",
    "@unhead/vue": "^2.0.12",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.13.5",
    "date-fns": "^3.6.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.548.0",
    "marked": "^15.0.11",
    "npm": "^11.5.1",
    "pinia": "^2.1.7",
    "sanitize-html": "^2.16.0",
    "unhead": "^2.0.12",
    "vue": "^3.4.19",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.3.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@vitejs/plugin-vue": "^6.0.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^7.2.2"
  }
}
